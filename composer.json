{
    "name": "thadbryson/org-laravel-nova",
    "type": "library",
    "description": "Laravel Nova setup for myself.",
    "license": "proprietary",
    "authors": [
        {
            "name": "Thad Bryson",
            "email": "thadbry@gmail.com",
            "homepage": "http://thadbryson.co",
            "role": "Creator"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        },
        {
            "type": "path",
            "url": "../data"
        }
    ],
    "require": {
        "php": ">=7.3",
        "ext-bcmath": "*",
        "ext-ctype": "*",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-mysqli": "*",
        "ext-openssl": "*",
        "ext-pdo": "*",
        "ext-tokenizer": "*",
        "ext-xml": "*",
        "davidpiesse/nova-phpinfo": "0.0.*",
        "digitalcloud/nova-custom-views": "1.*",
        "doctrine/dbal": "2.9.*",
        "gregoriohc/laravel-nova-theme-responsive": "0.6.*",
        "kabbouchi/nova-logs-tool": "0.1.*",
        "laraning/nova-time-field": "0.1.*",
        "laravel/framework": "5.8.*|6.*|7.*",
        "laravel/horizon": "4.0.*",
        "laravel/nova":  "2.3.*",
        "laravel/tinker": "2.2.*",
        "roave/security-advisories": "dev-master",
        "robertboes/nova-slider-field": "0.*",
        "sbine/route-viewer": "0.0.*",
        "spatie/nova-backup-tool": "3.3.*",
        "thadbryson/data": "*",
        "timothyasp/nova-color-field": "1.0.*",
        "vkovic/laravel-custom-casts": "1.1.*"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "barryvdh/laravel-ide-helper": "^2.6",
        "beyondcode/laravel-dump-server": "^1.0",
        "codeception/codeception": "^2.4",
        "davidhsianturi/laravel-compass": "^0.1.0",
        "deployer/deployer": "^6.5",
        "facade/ignition": "1.*",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "nunomaduro/phpinsights": "^1.0",
        "wnx/laravel-stats": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "TCB\\Laravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TCB\\Laravel\\ServiceProvider"
            ]
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
