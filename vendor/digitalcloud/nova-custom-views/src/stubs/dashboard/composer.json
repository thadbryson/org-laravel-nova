{
    "name": "nova-custom-views/dashboard-view",
    "description": "A Laravel Nova custom dashboard.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "NovaCustomViews\\DashboardView\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NovaCustomViews\\DashboardView\\DashboardViewServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
