{
    "name": "nova-custom-views/custom-404",
    "description": "A Laravel Nova custom 404.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "NovaCustomViews\\Custom404\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NovaCustomViews\\Custom404\\Custom404ServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
